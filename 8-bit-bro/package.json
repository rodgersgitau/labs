{
  "name": "8-bit-bro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "web": "pnpm dev",
    "dev:vite": "NODE_ENV=development vite --host --clearScreen false",
    "dev:vite:extract": "EXTRACT=1 pnpm dev",
    "dev": "pnpm dev:webpack",
    "dev:webpack": "DISABLE_EXTRACTION=true NODE_ENV=development webpack serve",
    "dev:extract": "DISABLE_EXTRACTION=false NODE_ENV=development webpack serve",
    "prod": "NODE_ENV=production webpack --json=dist/compilation-stats.json",
    "upgrade:tamagui": "pnpm up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "pnpm up '*tamagui*'@canary '@tamagui/*'@canary react-native-web-lite@canary"
  },
  "dependencies": {
    "@tamagui/animations-css": "latest",
    "@tamagui/core": "latest",
    "@tamagui/create-theme": "latest",
    "@tamagui/react-native-svg": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-web": "^0.18.10",
    "tamagui": "latest"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.19.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@tamagui/polyfill-dev": "^1.74.8",
    "@tamagui/vite-plugin": "latest",
    "@tamagui/web": "^1.74.8",
    "@types/node": "^20.8.1",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "async-retry": "1.3.1",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^5.2.4",
    "esbuild-loader": "^3.0.1",
    "esbuild-register": "^3.4.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.1",
    "tamagui-loader": "latest",
    "thread-loader": "^3.0.4",
    "url-loader": "^4.1.1",
    "vite": "^4.3.0",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}