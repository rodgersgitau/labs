{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/config/*": [
        "src/config/*"
      ],
      "@/data/*": [
        "src/data/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
    }
  }
}